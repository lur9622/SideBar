  }
}
@media(max-width:992px){
  .s_a_content >button {
    width:100%;
  }
  .s_o_element >div >button {
    width:80%;
  }
  .s_o_element >div >p {
    font-size:5vw;
  }
  .s_o_element {
    margin-bottom:25px;
  }
  #s_offer > div > div > div {
    padding: 0;
  }
  .s_aU_element>span{
