@media(max-width:767px){
  .n_menu_item {
    width:100%;
  }
  .s_aU_element>span{
    font-size:19px;
  }
  .s_aU_mockup>div>a>button {
    width:70%;
  }
  /* DAWID */
  #header_small p:first-child {
    line-height: 20px;
  }
  #header_small p {
    line-height: 72px;
  }
  .line_height_alter1 {
    line-height: 95px !important;
  }
  .navbar{
    padding: 20px 0;
  }
	.offer_text p:first-child {
    font-size: 28px;
    text-align: center;
  }
  #header_small {
    text-align: center;
  }
  #header_small p {
    font-size: 57px;
  }
  #header_small span {
    display: none;
  }
  #s_offer > div > div > div {
